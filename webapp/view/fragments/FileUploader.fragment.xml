<c:FragmentDefinition xmlns="sap.m"
	xmlns:c="sap.ui.core"
	xmlns:upload="sap.m.upload">>
	<Dialog id="iduploader" title="Carga de Ficheros" class="sapUiSizeCompact">
		<Panel width="auto" height="auto">
			<content>
				<HBox justifyContent="Center" alignContent="Center" alignItems="Center">
					<upload:UploadSet items="{path: '/AttachDocSet'}" id="attachmentUpl" instantUpload="true" maxFileSize="2" beforeUploadStarts="onStartUpload" selectionChanged="onSelectionChangeAttachment" uploadCompleted="onUploadCompleted" uploadUrl="/sap/opu/odata/sap/ZGWFI_COBRANZAS_SRV/AttachDocSet">
						<upload:toolbar>
							<OverflowToolbar>
								<CheckBox id="checkbox" select="onSelectAllAttachments" visible="false" />
								<ToolbarSpacer/>
								<Button id="remove" enabled="false" icon="sap-icon://delete" press="onRemove" type="Transparent" />
								<Button id="download" enabled="false" icon="sap-icon://download" press="onDownload" type="Transparent" />
							</OverflowToolbar>
						</upload:toolbar>
						<upload:items>
							<upload:UploadSetItem fileName="{Filename}" mediaType="{MimeType}" visibleEdit="false" visibleRemove="false" selected="true" url="/sap/opu/odata/sap/ZGWFI_COBRANZAS_SRV/AttachDocSet/$value">

							</upload:UploadSetItem>
						</upload:items>
					</upload:UploadSet>

				</HBox>
			</content>
		</Panel>
		<beginButton>
			<Button text="{i18n>btnvolver}" press="onCloseonFileDialog"/>

		</beginButton>
		<endButton>
			<!-- <Button id="upload" enabled="true" text="{i18n>btnguardar}" press="onStartUpload" type="Emphasized" visible="true" /> -->
		</endButton>
	</Dialog>
</c:FragmentDefinition>