<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:fb="sap.ui.comp.filterbar">

    <fb:FilterBar id="idFilterBar" header="{i18n>tabletitle}" reset="onFilterBarReset" showRestoreOnFB="true" search="onFilterBarSearch" showRestoreButton="false"   showFilterConfiguration="false">

        <fb:filterGroupItems>

            <fb:FilterGroupItem groupName="search" name="razonsocial" label="{i18n>lblrazonsocial} / {i18n>lblcuit}" visibleInFilterBar="true">
                <fb:control>
                  
                    <MultiInput id="idRazonSocialMultiInput" width="auto" showSuggestion="true" suggestionItems="{/CLIENTESSet}">
                        <core:Item key="{RazonSocial}" text="{RazonSocial} {Cuit}" />
                    </MultiInput>
                </fb:control>
            </fb:FilterGroupItem>

            <fb:FilterGroupItem groupName="search" name="cuit" label="{i18n>lblcuit}" visibleInFilterBar="false">
                <fb:control>
                    
                    <MultiInput id="idCuitMultiInput" width="auto" showSuggestion="true" suggestionItems="{/CLIENTESSet}">
                        <core:Item key="{Cuit}" text="{RazonSocial} {Cuit}"/>
                    </MultiInput>
                </fb:control>
            </fb:FilterGroupItem>

            <fb:FilterGroupItem groupName="search" name="fecha" label="{i18n>lblfecharango}" visibleInFilterBar="true">
                <fb:control>
                    <DateRangeSelection  id="idFechaDateRangeSelection" displayFormat="dd/MM/yyyy"/>                                          
                    
                </fb:control>
            </fb:FilterGroupItem>

            
            <fb:FilterGroupItem groupName="search" name="procesado" label="{i18n>lblprocesado}" visibleInFilterBar="true">
                <fb:control>
                    <ComboBox id="idProcesadoFilter"  width="60px" >
                        <core:Item key="X" text="SI" />
                        <core:Item key="null" text="NO" />
                    </ComboBox>
                    
                </fb:control>
            </fb:FilterGroupItem>


        </fb:filterGroupItems>

    </fb:FilterBar>

</core:FragmentDefinition>